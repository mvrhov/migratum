{
    "name": "mvrhov/migratum",
    "type": "library",
    "description": "Templated database migrations",
    "keywords": ["database migrations", "twig", "migrations", "database", "db", "migratum", "templated migrations", "sql migrations", "SQL"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Migratum\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "jdorn/sql-formatter": "^1.2",
        "league/container": "^3.0",
        "symfony/console": "^3.4|^4.0",
        "symfony/finder": "^3.4|^4.0",
        "twig/twig": "^2.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages":      true
    },
    "bin": ["bin/migratum"],
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
